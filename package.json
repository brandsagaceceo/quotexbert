    "@stripe/stripe-js": "^7.8.0",
    "crypto": "^1.0.1",
    "nanoid": "^5.1.5",
    "next": "15.4.6",
    "posthog-js": "^1.260.1",
    "posthog-node": "^5.7.0",
    "prisma": "^6.14.0",
    "puppeteer": "^24.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "resend": "^6.0.1",
    "stripe": "^18.4.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
